{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "arux",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "3000",
        "--web-renderer",
        "html",
        "--dart-define",
        "2"
      ]
    },
    {
      "name": "arux (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "50120",
        "--web-renderer",
        "html",
        "--dart-define",
        "2"
      ]
    },
    {
      "name": "arux (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--web-renderer", "html"]
    }
  ]
}
